
config LIRE_RACK_BUILD_SRC
    bool "    Build Rack"
    default y
    depends on LIRE_BUILD_SRC && LIRE_RACK_INST
    select LIRE_XENOMAI_BUILD_SRC
    select LIRE_OPENCV_BUILD_SRC if LIRE_RACK_OPENCV_SUPPORT
    select LIRE_LIBRAW1394_BUILD_SRC if LIRE_RACK_OPENCV_SUPPORT
    select LIRE_LIBDC1394_BUILD_SRC if LIRE_RACK_OPENCV_SUPPORT
    select LIRE_LIBPNG_BUILD_SRC if LIRE_RACK_OPENCV_SUPPORT
    select LIRE_LIBJPEG_BUILD_SRC if LIRE_RACK_OPENCV_SUPPORT
    select LIRE_RTNET_BUILD_SRC if LIRE_RACK_RTNET_SUPPORT
    select LIRE_GSL_BUILD_SRC if LIRE_RACK_GSL_SUPPORT
    select LIRE_ZLIB_BUILD_SRC if LIRE_RACK_ZLIB_SUPPORT
    select LIRE_LIBUSB_BUILD_SRC if LIRE_RACK_LIBUSB_SUPPORT
    select LIRE_BOOST_BUILD_SRC if LIRE_RACK_BOOST_SUPPORT
    select LIRE_PCL_BUILD_SRC if LIRE_RACK_PCL_SUPPORT
    help
